<openerp>
    <data>
        <act_window id="diane_diplomes"
                    name="Diplomes"
                    res_model="diane.diploma"
                    view_mode="tree,form"
                    limit="80"/>
        <menuitem id="diane.diploma" name="Diplomas" parent="base.menu_marketing_config_association"
                  action="diane_diplomes" sequence="15"/>

        <act_window id="diane_sections"
                    name="Sections"
                    res_model="diane.section"
                    view_mode="tree,form"
                    limit="80"/>
        <menuitem id="diane.section" name="Section" parent="base.menu_marketing_config_association"
                  action="diane_sections" sequence="15"/>

        <record id="form_all_customers" model="ir.ui.view">
            <field name="name">NACE</field>
            <field name="model">diane.nace</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="sequence" widget="handle"/>
                    <field name="code"/>
                    <field name="name"/>
                    <field name="level"/>
                    <field name="parent_id"/>
                </tree>
            </field>
        </record>
        <record id="diane_nace"
                model="ir.actions.act_window.view">
            <field name="act_window_id" ref="diane_nace"/>
            <field name="view_id" ref="diane_nace"/>
            <field name="view_mode">tree</field>
            <field name="sequence">10</field>
        </record>
        <act_window id="diane_nace"
                    name="NACE Codes"
                    res_model="diane.nace"
                    view_mode="tree,form"
                    limit="80"/>
        <menuitem id="diane.nace" name="NACE" parent="base.menu_marketing_config_association" action="diane_nace"
                  sequence="15"/>

    </data>
</openerp>